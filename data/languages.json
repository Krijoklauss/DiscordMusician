                    },
                    "fails": [
                        {
                            "title": "Error",
                            "description": "Failed to change the channel",
                            "color": [255, 0, 0],
                            "fields": [
                                {
                                    "name": "Response message", 
                                    "value": "The TEXTCHANNEL %s does not exist on this server!",
                                    "inline": false
                                }
                            ]
                        }
                    ]
                },
