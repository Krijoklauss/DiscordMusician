{
    "languages": {
        "en": {
            "commands": {
                "prefix": {
                    "works": {
                        "title": "Prefix change",
                        "description": "Current prefix has been changed",
                        "color": [255, 0, 255],
                        "fields": [
                            {
                                "name": "New Prefix", 
                                "value": "Your new prefix is now %s", 
                                "inline": false
                            },
                            {
                                "name": "Old Prefix", 
                                "value": "Your old prefix was %s", 
                                "inline": false
                            }
                        ]
                    },
                    "fails": [
                        {
                            "title": "Error",
                            "description": "Failed to change the prefix",
                            "color": [255, 0, 0],
                            "fields": [
                                {
                                    "name": "Response message", 
                                    "value": "Our predefined prefix pool does not contain your new prefix.\n This is our Prefix pool:\n%s",
                                    "inline": false
                                }
                            ]
                        }
                    ]
                },
                "bind": {
                    "works": {
                        "title": "Channel change",
                        "description": "Binded channel has been changed!",
                        "color": [255, 0, 255],
